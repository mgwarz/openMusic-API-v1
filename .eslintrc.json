{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "quotes": 0,
        "no-underscore-dangle": 0,
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": 0, 
        "class-methods-use-this": 0,
        "no-plusplus": 0
    }
}
